<h1>New Image</h1>
<%= simple_form_for @image, url: admin_images_path, html: { multipart: true } do |f| %>
  <fieldset>
    <%= f.input :picture, as: :file %>
    <div class="form-actions">
      <button class="btn btn-blue btn-form">Save Image</button>
      <%= link_to 'Cancel', admin_images_path, :class => "btn btn-form" %>
    </div>
  </fieldset>
<% end %>
