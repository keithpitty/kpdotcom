<h1>Testimonial Details</h1>

<fieldset>

  <%= f.input :rank %>
  <%= f.input :provider_name %>
  <%= f.input :provider_position %>
  <%= f.input :recommendation_year %>
  <%= f.input :recommendation, :input_html => { rows: "10" } %>

  <div class="form-actions">
    <button class="btn btn-blue btn-form">Save Testimonial</button>
    <%= link_to 'Cancel', admin_testimonials_path, :class => "btn btn-form" %>
  </div>

</fieldset>
