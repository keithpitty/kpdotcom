<h1>Achievement Details</h1>

<fieldset>

  <%= f.input :rank %>
  <%= f.input :heading %>
  <%= f.input :description, :input_html => { rows: "10" } %>

  <div class="form-actions">
    <button class="btn btn-blue btn-form">Save Achievement</button>
    <%= link_to 'Cancel', admin_achievements_path, :class => "btn btn-form" %>
  </div>

</fieldset>
